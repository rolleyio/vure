{
  "name": "@rolley/vure",
  "version": "1.0.0",
  "private": true,
  "description": "The firebase v9 application framework",
  "author": "Matt Rollinson <matt@rolley.io>",
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/es5/lib.umd.js",
  "module": "dist/lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    },
    "./": "./"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:lib && pnpm build:types",
    "build:lib": "vite build",
    "build:types": "rollup --config ../../config/rollup.types.config.js",
    "tsc": "vue-tsc src --noEmit",
    "tsc:w": "vue-tsc  src --noEmit --watch",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "test": "jest --config ../../jest.config.ts --runInBand",
    "test:w": "jest --config ../../jest.config.ts --watch --runInBand",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.4"
  }
}
