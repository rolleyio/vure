{
  "name": "@rolley/vure",
  "version": "0.1.2",
  "main": "./dist/vure.umd.js",
  "module": "./dist/vure.es.js",
  "exports": {
    ".": {
      "import": "./dist/vure.es.js",
      "require": "./dist/vure.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "build": "vuedx-typecheck . && vite build",
    "watch-functions": "cd functions && npm run watch",
    "emulators": "npm run watch-functions & firebase emulators:start --inspect-functions --import=./seeds --export-on-exit & wait",
    "indexes": "firebase firestore:indexes",
    "test": "jest",
    "test:rules": "jest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "firebase": "^9.0.0-beta.6",
    "vue": "next",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@firebase/rules-unit-testing": "^1.3.7",
    "@rollup/plugin-typescript": "^8.2.3",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "next",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "volta": {
    "node": "15.6.0",
    "npm": "7.20.2"
  }
}
